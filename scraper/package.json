    "test:local": "wrangler dev --local",
    "logs": "wrangler tail"
  },
  "dependencies": {
    "hono": "^4.6.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250819.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.4.0",
    "wrangler": "^4.30.0"
  }
}
