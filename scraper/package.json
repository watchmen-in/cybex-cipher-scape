{
  "name": "cydex-scraper",
  "version": "1.0.0",
  "description": "Federal CI entity web scraper for CyDex platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "build": "tsc && node build.js",
    "deploy": "npm run build && wrangler deploy",
